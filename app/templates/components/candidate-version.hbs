<section class="infos m--margin-Left-15">
  <h5 class="h5 m--margin-bottom-5"> {{version.make.name}} - {{version.submodel.name}} {{#if candidate.suggested}}<i class="la la-star m--font-info"></i>{{/if}}</h5>
  <p class="m--margin-bottom-5">{{t "application.gen"}} {{version.generation.name}} {{t "application.ph"}} {{version.phase.name}}</p>
  <p class="m--marginless">{{version.name}}</p>
</section>
<div class="logo-img">
  {{logo-img data-src=version.make.logoURL className="thumbnail make-logo"}}
</div>
<div class="candidate-frequency m--margin-Left-15">

  <div
    class="candidate-frequency__gauge {{candidateGaugeColor}}"
    {{! template-lint-disable }}
    style={{{candidateFrequencyStyle}}}
  >
  </div>
</div>
